                <div class="icons_with_text_item">
                <div class="icons_with_text_item_flex">
                  <div class="icons_with_text_item_svg">
                    {% if block.settings.custom_class contains 'custom_counter' %}
                      <span class="count_js" data-value="{{- block.settings.svg1 | replace:'%','' -}}">{{ block.settings.svg1 | replace:'%','' }}</span>%
                    {% else %}
                  {{ block.settings.svg1 }}
                    {% endif %}
                  </div>
                  {% if block.settings.image1 != blank %}
                  <img src="{{ block.settings.image1 | image_url }}" width="auto" height="auto" fetchpriority="{{ block.settings.fetchpriority }}" loading="{{ block.settings.loading }}" alt="{{ block.settings.image1.alt }}">  
                  {% endif %}
                  <div class="icons_with_text_item_title">
                  {{ block.settings.title1 }}
                  </div>
                 </div>
                <div class="icons_with_text_item_text">{{ block.settings.text1 }}</div>
                 </div>
                </div>
                  {% endif %}
                  {% if block.settings.text2 != blank %}
