                  {% endif %}
                </div>
                <div class="image_with_text_item_right">
                  {% if block.settings.image2 != blank %}
                  <img src="{{ block.settings.image2 | image_url }}" width="auto" height="auto" fetchpriority="{{ block.settings.fetchpriority }}" loading="{{ block.settings.loading }}" alt="{{ block.settings.image2.alt }}">  
                  {% endif %}
                  {% if block.settings.text2 != blank %}
                    <div class="image_with_text_item_text">{{ block.settings.text2 }}</div>
                  {% endif %}
                </div>
                 </div>
              </div>
            {% endif %}
            {% when 'image_with_text2' %}
                  {% if block.settings.image != blank %}
              <div  class="custom_image2_with_text_landing_page {{ block.settings.custom_class }}">
                <div class="image2_with_text_item">
                <div class="image2_with_text_item_left left">
                  {% if block.settings.image != blank %}
                  <img src="{{ block.settings.image | image_url }}" width="auto" height="auto" fetchpriority="{{ block.settings.fetchpriority }}" loading="{{ block.settings.loading }}" alt="{{ block.settings.image.alt }}">  
                  {% endif %}
                </div>
                <div class="image2_with_text_item_right">
                  {% if block.settings.badge != blank %}
                    <div class="image2_with_text_item_badge">{{ block.settings.badge }}</div>
                  {% endif %}
                  {% if block.settings.title != blank %}
                    <div class="image2_with_text_item_title h2">{{ block.settings.title }}</div>
                  {% endif %}
                  {% if block.settings.text != blank %}
                    <div class="image2_with_text_item_text">{{ block.settings.text }}</div>
                  {% endif %}
                  {% if block.settings.image2 != blank %}
                      <div class="image2_with_text_item_pic">
                  <img src="{{ block.settings.image2 | image_url }}" width="auto" height="auto" fetchpriority="{{ block.settings.fetchpriority }}" loading="{{ block.settings.loading }}" alt="{{ block.settings.image2.alt }}">  
                      </div>
                  {% endif %}
                  {% if block.settings.review_text != blank %}
                   <div class="reviews_column_rating_html">
              <a href="#reviews">
             {{ block.settings.svg }}
             {{ block.settings.review_text }}
             </a>
            </div>
                  {% endif %}
                </div>
                 </div>
              </div>
            {% endif %}
         
            {% when 'icon_with_text' %}
              <div  class="custom_icon_with_text_landing_page {{ block.settings.custom_class }}">
                <div class="icon_with_text_flex">
                <div class="icon_with_text_item">
                  {{ block.settings.svg1 }}
                  {% if block.settings.image1 != blank %}
                  <img src="{{ block.settings.image1 | image_url }}" width="auto" height="auto" fetchpriority="{{ block.settings.fetchpriority }}" loading="{{ block.settings.loading }}" alt="{{ block.settings.image1.alt }}">  
                  {% endif %}
                  {{ block.settings.text1 }}
                 </div>
                 </div>
              </div>
            


            {% when 'after_before_slider' %}
            {% if after_before == false %}
          {% assign after_before = true %}
<div class="after_before_slider_main_div">
       <div class="after_before_slider_main_slider_new_block slider_{{ block.id }} with_buttons custom_slick_slider">
               {%- for block in section.blocks -%}
          {%- case block.type -%}
            {% when 'after_before_slider' %}
              <div  class="after_before_with_text_landing_page {{ block.settings.custom_class }}">
                <div class="after_before_flex">
                <div class="after_before_with_text_item">
          {% if block.settings.image1 != blank %}
              <div class="step_image">
             <img src="{{ block.settings.image1 | image_url }}" width="auto" height="auto" fetchpriority="{{ block.settings.fetchpriority }}" loading="{{ block.settings.loading }}" alt="{{ block.settings.image1.alt }}">  
             </div>
            {% endif %}
                 <div class="after_before_title"> {{ block.settings.title1 }}</div>
                 <div class="after_before_text">{{ block.settings.text1 }}</div>
                 </div>
                <div class="after_before_with_text_item">
          {% if block.settings.image2 != blank %}
              <div class="step_image">
             <img src="{{ block.settings.image2 | image_url }}" width="auto" height="auto" fetchpriority="{{ block.settings.fetchpriority }}" loading="{{ block.settings.loading }}" alt="{{ block.settings.image2.alt }}">  
             </div>
            {% endif %}
                 <div class="after_before_title"> {{ block.settings.title2 }}</div>
                 <div class="after_before_text">{{ block.settings.text2 }}</div>
                 </div>
                 </div>
              </div>

    {% endcase %}
       {% endfor %} 
       </div>


