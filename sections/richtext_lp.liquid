                  <div class="icons_with_text_item_svg">
                    {% if block.settings.custom_class contains 'custom_counter' %}
                      <span class="count_js" data-value="{{- block.settings.svg2 | replace:'%','' -}}">{{ block.settings.svg2 | replace:'%','' }}</span>%
                    {% else %}
                  {{ block.settings.svg2 }}
                    {% endif %}
                  </div>
                  {% if block.settings.image2 != blank %}
                  <img src="{{ block.settings.image2 | image_url }}" width="auto" height="auto" fetchpriority="{{ block.settings.fetchpriority }}" loading="{{ block.settings.loading }}" alt="{{ block.settings.image2.alt }}">  
                  {% endif %}
                  <div class="icons_with_text_item_title">
                  {{ block.settings.title2 }}
                  </div>
                 </div>
                <div class="icons_with_text_item_text">{{ block.settings.text2 }}</div>
                 </div>
                </div>
                  {% endif %}
                  {% if block.settings.text3 != blank %}
                <div class="icons_with_text_flex">
                <div class="icons_with_text_item">
                <div class="icons_with_text_item_flex">
                  <div class="icons_with_text_item_svg">
                    {% if block.settings.custom_class contains 'custom_counter' %}
                      <span class="count_js" data-value="{{- block.settings.svg3 | replace:'%','' -}}">{{ block.settings.svg3 | replace:'%','' }}</span>%
                    {% else %}
                  {{ block.settings.svg3 }}
                    {% endif %}
                  </div>
                  {% if block.settings.image3 != blank %}
                  <img src="{{ block.settings.image3 | image_url }}" width="auto" height="auto" fetchpriority="{{ block.settings.fetchpriority }}" loading="{{ block.settings.loading }}" alt="{{ block.settings.image3.alt }}">  
                  {% endif %}
                  <div class="icons_with_text_item_title">
                  {{ block.settings.title3 }}
                  </div>
                 </div>
                <div class="icons_with_text_item_text">{{ block.settings.text3 }}</div>
                 </div>
                </div>
                  {% endif %}
                  {% if block.settings.text4 != blank %}
                <div class="icons_with_text_flex">
                <div class="icons_with_text_item">
                <div class="icons_with_text_item_flex">
                  <div class="icons_with_text_item_svg">
                  {{ block.settings.svg4 }}
                  </div>
                  {% if block.settings.image4 != blank %}
                  <img src="{{ block.settings.image4 | image_url }}" width="auto" height="auto" fetchpriority="{{ block.settings.fetchpriority }}" loading="{{ block.settings.loading }}" alt="{{ block.settings.image4.alt }}">  
                  {% endif %}
                  <div class="icons_with_text_item_title">
                  {{ block.settings.title4 }}
                  </div>
                 </div>
                <div class="icons_with_text_item_text">{{ block.settings.text4 }}</div>
                 </div>
                </div>
                  {% endif %}
                 </div>
            
         
            {% when 'steps' %}
<div class="steps_slider_main_div">
       <div class="steps_slider_main_slider_new_flex">
              <div  class="custom_steps_with_text_landing_page {{ block.settings.custom_class }}">
                <div class="steps_flex">
                <div class="steps_with_text_item">
                  {% if block.settings.image1 != blank %}
                    <div class="steps_with_text_item_image">
                  <img src="{{ block.settings.image1 | image_url }}" width="auto" height="auto" fetchpriority="{{ block.settings.fetchpriority }}" loading="{{ block.settings.loading }}" alt="{{ block.settings.image1.alt }}">  
                    </div>
                  {% endif %}
                 <div class="step"> {{ block.settings.title1 }}</div>
                 <div class="step_text">{{ block.settings.text1 }}</div>
                 </div>
                <div class="steps_with_text_item">
                  {% if block.settings.image2 != blank %}
                    <div class="steps_with_text_item_image">
                  <img src="{{ block.settings.image2 | image_url }}" width="auto" height="auto" fetchpriority="{{ block.settings.fetchpriority }}" loading="{{ block.settings.loading }}" alt="{{ block.settings.image2.alt }}">  
                    </div>
                  {% endif %}
                 <div class="step"> {{ block.settings.title2 }}</div>
                 <div class="step_text">{{ block.settings.text2 }}</div>
                 </div>
                <div class="steps_with_text_item">
                  {% if block.settings.image3 != blank %}
                    <div class="steps_with_text_item_image">
                  <img src="{{ block.settings.image3 | image_url }}" width="auto" height="auto" fetchpriority="{{ block.settings.fetchpriority }}" loading="{{ block.settings.loading }}" alt="{{ block.settings.image3.alt }}">  
                    </div>
                  {% endif %}
                 <div class="step"> {{ block.settings.title3 }}</div>
                 <div class="step_text">{{ block.settings.text3 }}</div>
                 </div>
                 </div>
              </div>
       </div>

</div>

            
         
            {% when 'image_with_text' %}
                  {% if block.settings.image != blank %}
              <div  class="custom_icons_with_text_landing_page {{ block.settings.custom_class }}">
                <div class="image_with_text_item">
                <div class="image_with_text_item_left left">
                  {% if block.settings.title != blank %}
                    <div class="image_with_text_item_title h2">{{ block.settings.title }}</div>
                  {% endif %}
                  {% if block.settings.text != blank %}
                    <div class="image_with_text_item_text">{{ block.settings.text }}</div>
                  {% endif %}
                </div>
                <div class="image_with_text_item_right">
                  {% if block.settings.image != blank %}
                  <img src="{{ block.settings.image | image_url }}" width="auto" height="auto" fetchpriority="{{ block.settings.fetchpriority }}" loading="{{ block.settings.loading }}" alt="{{ block.settings.image.alt }}">  
                  {% endif %}
                </div>
                 </div>
              </div>
            {% endif %}
    
            {% when 'two_image_with_text' %}
                  {% if block.settings.image1 != blank %}
              <div  class="custom_icons_with_text_landing_page {{ block.settings.custom_class }}">
                <div class="image_with_text_item">
                <div class="image_with_text_item_left left">
                  {% if block.settings.image1 != blank %}
                  <img src="{{ block.settings.image1 | image_url }}" width="auto" height="auto" fetchpriority="{{ block.settings.fetchpriority }}" loading="{{ block.settings.loading }}" alt="{{ block.settings.image1.alt }}">  
                  {% endif %}
