    },
    {
      "type": "range",
      "id": "padding_top_mobile",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "label": "Padding Top Mobile",
      "default": 50
    },
    {
      "type": "range",
      "id": "padding_bottom_mobile",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "label": "Padding Bottom Mobile",
      "default": 50
    }
    ],
  "blocks": [
    {
      "type": "heading",
      "name": "Heading",
      "settings": [
        {
          "type": "inline_richtext",
          "id": "text",
          "label": "Text"
        },
        {
          "type": "select",
          "id": "heading_size",
          "options": [
            {
              "value": "h2",
              "label": "Small"
            },
            {
              "value": "h1",
              "label": "Medium"
            },
            {
              "value": "h0",
              "label": "Large"
            }
          ],
          "default": "h1",
          "label": "Heading Size"
        },
         {
          "type": "checkbox",
          "id": "h1",
          "label": "Use H1 tag"
        },
         {
      "type": "text",
      "id": "custom_class",
      "label": "Custom Class"
    }
      ]
    },
    {
      "type": "text",
      "name": "Text",
      "settings": [
        {
          "type": "inline_richtext",
          "id": "text",
          "default": "Share information about your brand with your customers. Describe a product, make announcements, or welcome customers to your store.",
          "label": "Text"
        },
         {
      "type": "text",
      "id": "custom_class",
      "label": "Custom Class"
    }
      ]
    },
    {
      "type": "richtext",
      "name": "RichText",
      "settings": [
        {
          "type": "richtext",
          "id": "text",
          "default": "<p>Share information about your brand with your customers. Describe a product, make announcements, or welcome customers to your store.</p>",
          "label": "Text"
        },
         {
      "type": "text",
      "id": "custom_class",
      "label": "Custom Class"
    }
      ]
    },
    {
      "type": "html",
      "name": "Html",
      "settings": [
        {
          "type": "liquid",
          "id": "text",
          "label": "Html"
        },
         {
      "type": "text",
      "id": "custom_class",
      "label": "Custom Class"
    }
      ]
    },
